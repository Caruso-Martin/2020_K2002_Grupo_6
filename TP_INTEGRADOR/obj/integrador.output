   24 expresionIgualdad: expresionRelacional
   25                  | expresionIgualdad OPERADOR_IGUALDAD expresionRelacional

   26 expresionRelacional: expresionCorrimiento
   27                    | expresionRelacional OPERADOR_RELACIONAL expresionCorrimiento

   28 expresionCorrimiento: expresionAditiva
   29                     | expresionCorrimiento OPERADOR_CORRIMIENTO expresionAditiva

   30 expresionAditiva: expresionMultiplicativa
   31                 | expresionAditiva '+' expresionMultiplicativa
   32                 | expresionAditiva '-' expresionMultiplicativa

   33 expresionMultiplicativa: expresionConversion
   34                        | expresionMultiplicativa '*' expresionConversion
   35                        | expresionMultiplicativa OPERADOR_MULTIPLICATIVO expresionConversion

   36 expresionConversion: expresionUnaria
   37                    | '(' TIPO_DATO ')' expresionConversion

   38 expresionUnaria: expresionSufijo
   39                | OPERADOR_INCREMENTO expresionUnaria
   40                | operadorUnario expresionConversion
   41                | OPERADOR_SIZEOF expresionUnaria
   42                | OPERADOR_SIZEOF '(' TIPO_DATO ')'

   43 operadorUnario: '&'
   44               | '*'
   45               | '+'
   46               | '-'
   47               | '~'
   48               | '!'

   49 expresionSufijo: expresionPrimaria
   50                | expresionSufijo '[' expresion ']'
   51                | expresionSufijo '(' listaArgumentos_ ')'
   52                | expresionSufijo OPERADOR_SELECCION_MIEMBRO IDENTIFICADOR
   53                | expresionSufijo OPERADOR_INCREMENTO

   54 listaArgumentos: expresionAsignacion
   55                | listaArgumentos ',' expresionAsignacion

   56 expresionPrimaria: IDENTIFICADOR
